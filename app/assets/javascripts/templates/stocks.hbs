
<div class="span3">
  <div class="well">
    <ul class="nav nav-list">
      <li class="nav-header">All Stocks ( {{with_ids.length}} )</li>
      {{#each stock in with_ids}}
        {{view HelloEmber.StockInListView contentBinding="stock"}}
      {{/each}}
    </ul>
  </div>
</div>

<div class="span9">
	
<div id="flash" class="alert alert-success">
    <span></span>
</div>
	
{{#if activeStockId}}
{{else}}
<p class="box">Stocks {{#linkTo "stocks.new" class="btn btn-mini" }}<i class="icon-plus-sign"></i> Add Stock{{/linkTo}}</p>
{{/if}}

{{outlet}}

{{#if activeStockId}}
{{else}}
	{{#if controller.with_ids}}
		<table class='table table-condensed table-striped table-bordered'>
			{{partial "partials/stock_table_header"}}
		{{#each stock in with_ids}}
			{{partial "partials/stock"}}
		  	{{/each}}
			<tr>
			<td></td>
			<td></td>
			<th style='text-align:right;'>Total</th>
			<td style='text-align:right;'>${{decimal total_cost}}</td>
			<td></td>
			<td style='text-align:right;'>${{decimal total_value}}</td>
			<td></td>
			<td></td>
			<td></td>
			</tr>
		</table>
	{{/if}}
{{/if}}

</div>


